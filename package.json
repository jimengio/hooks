{
  "name": "@jimengio/jimo-hooks",
  "version": "0.2.0-alpha",
  "description": "Some react hooks",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "rm -rf ./lib/ && tsc -d --project tsconfig.json --outDir ./lib/",
    "test": "jest -c jest.json",
    "lint": "eslint 'src/**/*.{ts,tsx}'",
    "prettier": "prettier --write './src/**/*.{ts,tsx}' --config ./.prettierrc",
    "prepare": "yarn build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jimengio/jimo-hooks.git"
  },
  "keywords": [
    "react",
    "hooks",
    "react-hooks"
  ],
  "author": "wangcch <wangcch.cc@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "lodash.isequal": "^4.5.0",
    "use-debounce": "^3.4.1"
  },
  "peerDependencies": {
    "react": "^16.8.0",
    "react-dom": "^16.8.0"
  },
  "devDependencies": {
    "@testing-library/react-hooks": "^3.1.0",
    "@types/enzyme": "^3.10.3",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/jest": "^25.2.1",
    "@types/lodash.isequal": "^4.5.5",
    "@types/react": "^16.9.9",
    "@types/react-dom": "^16.9.2",
    "@types/react-transition-group": "^4.2.3",
    "@typescript-eslint/eslint-plugin": "^2.23.0",
    "@typescript-eslint/parser": "^2.23.0",
    "emotion": "^10.0.17",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^2.5.0",
    "gh-pages": "^2.1.1",
    "husky": "^4.2.3",
    "jest": "^25.3.0",
    "lint-staged": "^10.0.8",
    "prettier": "^1.19.1",
    "react": "^16.8.0",
    "react-dom": "^16.8.0",
    "react-test-renderer": "^16.11.0",
    "react-transition-group": "^4.3.0",
    "ts-jest": "^25.3.1",
    "typescript": "^3.7.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{ts,tsx,js}": [
      "eslint --fix",
      "git add"
    ]
  }
}
